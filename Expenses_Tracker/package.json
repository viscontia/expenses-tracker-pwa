  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@tanstack/eslint-plugin-router": "^1.115.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.4",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.1",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
