{
  "name": "first-project",
  "scripts": {
    "postinstall": "prisma generate && tsr generate",
    "dev": "kill-port 3000 && concurrently \"pnpm:tsr-watch\" \"pnpm:vinxi-dev\"",
    "tsr-watch": "tsr generate -w",
    "vinxi-dev": "vinxi dev --port 3000",
    "build": "./build.sh",
    "start": "vinxi start",
    "test": "vitest",
    "test:run": "vitest run",
    "update-rates": "tsx src/server/scripts/update-exchange-rates.ts",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . -c eslint.config.mjs --max-warnings 0",
    "db:generate": "prisma migrate dev",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "format": "prettier --write .",
    "test-exchange": "tsx src/server/scripts/test-exchange-update.ts",
    "update-exchange": "tsx src/server/scripts/manual-update-exchange.ts",
    "migrate-historical": "tsx src/server/scripts/migrate-historical-rates.ts migrate",
    "migrate-rollback": "tsx src/server/scripts/migrate-historical-rates.ts rollback",
    "migrate-status": "tsx src/server/scripts/migrate-historical-rates.ts status",
    "migrate-monitor": "tsx src/server/scripts/migration-monitor.ts",
    "create-admin": "tsx src/server/scripts/create-admin.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.3",
    "@hookform/resolvers": "^5.1.1",
    "@prisma/client": "^6.8.2",
    "@tailwindcss/forms": "^0.5.10",
    "@tanstack/react-query": "^5.66.0",
    "@tanstack/react-router": "^1.119.0",
    "@tanstack/react-start": "^1.119.0",
    "@tanstack/router-cli": "^1.119.0",
    "@tanstack/router-plugin": "^1.119.0",
    "@trpc/client": "^11.1.2",
    "@trpc/react-query": "^11.4.3",
    "@trpc/server": "^11.1.2",
    "@types/pg": "^8.15.4",
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "dotenv": "^17.2.0",
    "html2canvas": "^1.4.1",
    "jsonwebtoken": "^9.0.2",
    "jspdf": "^3.0.1",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.510.0",
    "minio": "^8.0.5",
    "pg": "^8.16.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.58.1",
    "react-hot-toast": "^2.5.2",
    "superjson": "^2.2.2",
    "tailwindcss": "^3.4.17",
    "vinxi": "0.5.3",
    "vite-plugin-node-polyfills": "^0.24.0",
    "zod": "^3.25.0",
    "zustand": "^5.0.6"
